{
  "trailingSlash": true,
  "redirects": [
    { 
      "source": "/desktop/conduktor-first-steps/install",
      "destination": "/platform/category/get-started/", 
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "developers.conduktor.io"
        }
      ],
      "destination": "/developers/:path*"
    }
  ]
}

 