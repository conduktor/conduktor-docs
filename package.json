{
  "name": "conduktor-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "prestart": "yarn run build:stitches",
    "start": "docusaurus start",
    "prebuild": "yarn run build:stitches",
    "build": "docusaurus build && yarn run build:algolia",
    "build:algolia": "node algolia.index.mjs",
    "build:stitches": "cd ./src/styles && tsup index.ts && cd ../../ && node stitches.js",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\" --prose-wrap never",
    "postinstall": "yarn run build:stitches"
  },
  "dependencies": {
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/plugin-client-redirects": "^3.7.0",
    "@docusaurus/plugin-content-docs": "^3.7.0",
    "@docusaurus/plugin-google-analytics": "^3.7.0",
    "@docusaurus/plugin-google-gtag": "^3.7.0",
    "@docusaurus/preset-classic": "^3.7.0",
    "@docusaurus/theme-mermaid": "^3.7.0",
    "@mdx-js/react": "^3.1.0",
    "@radix-ui/colors": "^0.1.8",
    "@stitches/react": "^1.2.8",
    "algoliasearch": "^4.14.2",
    "clsx": "^1.2.1",
    "docusaurus-gtm-plugin": "^0.0.2",
    "docusaurus-plugin-dotenv": "^1.0.1",
    "docusaurus-plugin-sass": "^0.2.2",
    "docusaurus2-dotenv": "^1.4.0",
    "prism-react-renderer": "^2.1.0",
    "react": "^19.0.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^19.1.0",
    "react-hubspot-form": "^1.3.7",
    "sass": "^1.56.0",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.23.2",
    "@docusaurus/module-type-aliases": "^3.7.0",
    "@docusaurus/tsconfig": "^3.7.0",
    "@docusaurus/types": "^3.7.0",
    "dotenv": "^16.0.3",
    "fs": "^0.0.1-security",
    "globby": "^13.1.2",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.2",
    "prettier": "^2.7.1",
    "prompt-sync": "^4.2.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.2.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=18.x",
    "yarn": ">=1.22.x",
    "pnpm": "yarn",
    "npm": "yarn"
  }
}
